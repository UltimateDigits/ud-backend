{
  "name": "@huddle01/server-sdk",
  "version": "2.2.2",
  "description": "The Huddle01 Server SDK allows you to perform protected admin actions on your server side, like generating peer access tokens and starting and stopping meeting recordings and livestreams.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    "./webhooks": {
      "import": "./dist/webhooks/index.js",
      "require": "./dist/webhooks/index.cjs",
      "types": "./dist/webhooks/index.d.ts"
    },
    "./auth": {
      "import": "./dist/auth/index.js",
      "require": "./dist/auth/index.cjs",
      "types": "./dist/auth/index.d.ts"
    },
    "./recorder": {
      "import": "./dist/recorder/index.js",
      "require": "./dist/recorder/index.cjs",
      "types": "./dist/recorder/index.d.ts"
    },
    "./client": {
      "import": "./dist/client/index.js",
      "require": "./dist/client/index.cjs",
      "types": "./dist/client/index.d.ts"
    },
    "./api": {
      "import": "./dist/api/index.js",
      "require": "./dist/api/index.cjs",
      "types": "./dist/api/index.d.ts"
    }
  },
  "scripts": {
    "build": "rm -rf dist && tsup",
    "watch": "tsup --watch",
    "test": "yarn vitest run --typecheck",
    "pub": "yarn build && npm publish --access public",
    "pub:beta": "yarn build && npm publish --access public --tag beta",
    "pub:alpha": "yarn build && npm publish --access public --tag alpha",
    "pub:dev": "yarn build && npm publish --access public --tag dev"
  },
  "keywords": [
    "huddle01",
    "server",
    "sdk",
    "recorder",
    "livestream",
    "webhooks",
    "api",
    "client"
  ],
  "files": [
    "dist/*",
    "index.d.ts",
    "auth.d.ts",
    "recorder.d.ts",
    "webhooks.d.ts",
    "client.d.ts",
    "api.d.ts"
  ],
  "author": "a4748g",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^20.5.0",
    "tsconfig": "workspace:*",
    "tsup": "^8.0.1",
    "types": "workspace:*",
    "typescript": "^5.2.2",
    "vitest": "^1.2.0"
  },
  "peerDependencies": {
    "axios": "^1.6.7",
    "jose": "^4.14.6",
    "zod": "^3.22.4"
  },
  "dependencies": {
    "axios": "^1.6.7",
    "jose": "^4.14.6",
    "zod": "^3.22.4"
  }
}
